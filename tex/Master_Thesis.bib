	pages = {093012},
	file = {IOP Full Text PDF:C\:\\Users\\johan\\Zotero\\storage\\VBJ2ZJ99\\Aumann et al. - 2022 - CircuitQ an open-source toolbox for superconducti.pdf:application/pdf},
}

