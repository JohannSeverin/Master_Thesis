@article{nguyen_blueprint_2022,
	title = {Blueprint for a {High}-{Performance} {Fluxonium} {Quantum} {Processor}},
	volume = {3},
	url = {https://link.aps.org/doi/10.1103/PRXQuantum.3.037001},
	doi = {10.1103/PRXQuantum.3.037001},
	abstract = {Transforming stand-alone qubits into a functional, general-purpose quantum processing unit requires an architecture where many-body quantum entanglement can be generated and controlled in a coherent, modular, and measurable fashion. Electronic circuits promise a well-developed pathway for large-scale integration once a mature library of quantum-compatible elements have been developed. In the domain of superconducting circuits, fluxonium has recently emerged as a promising qubit due to its high-coherence and large anharmonicity, yet its scalability has not been systematically explored. In this work, we present a blueprint for a high-performance fluxonium-based quantum processor that addresses the challenges of frequency crowding, and both quantum and classical crosstalk. The main ingredients of this architecture include high-anharmonicity circuits, multipath couplers to entangle qubits where spurious longitudinal coupling can be nulled, circuit designs that are compatible with multiplexed microwave circuitry, and strongly coupled readout channels that do not require complex, frequency-sculpted elements to maintain coherence. In addition, we explore robust and resource-efficient protocols for quantum logical operations, then perform numerical simulations to validate the expected performance of this proposed processor with respect to gate fidelity, fabrication yield, and logical error suppression. Lastly, we discuss practical considerations to implement the architecture and achieve the anticipated performance.},
	number = {3},
	urldate = {2022-12-12},
	journal = {PRX Quantum},
	author = {Nguyen, Long B. and Koolstra, Gerwin and Kim, Yosep and Morvan, Alexis and Chistolini, Trevor and Singh, Shraddha and Nesterov, Konstantin N. and Jünger, Christian and Chen, Larry and Pedramrazi, Zahra and Mitchell, Bradley K. and Kreikebaum, John Mark and Puri, Shruti and Santiago, David I. and Siddiqi, Irfan},
	month = aug,
