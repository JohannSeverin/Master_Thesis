@article{riesch_analyzing_2019,
	title = {Analyzing the positivity preservation of numerical methods for the {Liouville}-von {Neumann} equation},
	volume = {390},
	issn = {0021-9991},
	url = {https://www.sciencedirect.com/science/article/pii/S0021999119302396},
	doi = {10.1016/j.jcp.2019.04.006},
	abstract = {The density matrix is a widely used tool in quantum mechanics. In order to determine its evolution with respect to time, the Liouville-von Neumann equation must be solved. However, analytic solutions of this differential equation exist only for simple cases. Additionally, if the equation is coupled to Maxwell's equations to model light-matter interaction, the resulting equation set – the Maxwell-Bloch or Maxwell-Liouville-von Neumann (MLN) equations – becomes nonlinear. In these advanced cases, numerical methods are required. Since the density matrix has certain mathematical properties, the numerical methods applied should be designed to preserve those properties. We establish the criterion that only methods that have a completely positive trace preserving (CPTP) update map can be used in long-term simulations. Subsequently, we assess the three most widely used methods – the matrix exponential (ME) method, the Runge-Kutta (RK) method, and the predictor-corrector (PC) approach – whether they provide this feature, and demonstrate that only the update step of the matrix exponential method is a CPTP map.},
	language = {en},
	urldate = {2022-11-17},
	journal = {Journal of Computational Physics},
	author = {Riesch, Michael and Jirauschek, Christian},
	month = aug,
	year = {2019},
	keywords = {Density matrix, Liouville-von Neumann equation, Maxwell-Liouville-von Neumann (MLN) equations, Numerical methods, Quantum mechanics},
	pages = {290--296},
	file = {ScienceDirect Snapshot:C\:\\Users\\johan\\Zotero\\storage\\FSXMK4V4\\S0021999119302396.html:text/html;Submitted Version:C\:\\Users\\johan\\Zotero\\storage\\AVJCW3BS\\Riesch and Jirauschek - 2019 - Analyzing the positivity preservation of numerical.pdf:application/pdf},
}

