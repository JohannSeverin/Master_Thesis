@misc{swiadek_enhancing_2023,
	title = {Enhancing {Dispersive} {Readout} of {Superconducting} {Qubits} {Through} {Dynamic} {Control} of the {Dispersive} {Shift}: {Experiment} and {Theory}},
	shorttitle = {Enhancing {Dispersive} {Readout} of {Superconducting} {Qubits} {Through} {Dynamic} {Control} of the {Dispersive} {Shift}},
